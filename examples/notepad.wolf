let lines: list<string> = []

fn write()
    let i: int = 0
    while 0 < 1
        let in: string = input(">")
        if in == "all()"
            read()
        end
        lines.push(in)
    end
end

fn read()
    let line_length: int = lines.len()
    # Gereksiz atama kaldırıldı: line_length = line_length
    print line_length
    for int i = 0 range line_length
        print lines[i]
    end
end

write()